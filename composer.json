{
    "name": "naftali100/async_bot",
    "type": "library",
    "description": "async telegram bots library",
    "keywords": [
        "async-telegram-bots"
    ],
    "homepage": "https://github.com/naftali100/async_tgbot",
    "license": "MIT",
    "require": {
        "amphp/amp": "^2.6.2",
        "amphp/http": "^1.6.3",
        "amphp/http-client": "^4.6.2",
        "amphp/http-server": "^2.1.7",
        "amphp/log": "^1.1",
        "amphp/cluster": "^1.0.1",
        "amphp/file": "^2.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.24",
        "amphp/phpunit-util": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "bot_lib\\": "./src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
